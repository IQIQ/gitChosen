  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  
  
  #use_frameworks!
  

  pod 'Alamofire', '~> 4.5'
  #pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git'
  pod 'Alamofire-SwiftyJSON'
  pod 'SwiftyJSON'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Charts'
  

  # Pods for BigProject

  target 'BigProjectTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BigProjectUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
